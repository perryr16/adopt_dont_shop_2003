<div class="background">
  <img src="https://wallpaperaccess.com/full/783057.jpg" alt="kitten">
</div>

<div class="content">
  <div class="main_menu">
    <a href="/">Main</a>
    <a href="/shelters">Shelters</a>
    <a href="/shelters/new">New Shelter</a>
    <a href="/pets">Pets</a>

  </div>

  <h1 class="header">Update A Shelter</h1>

  <form action="/shelters/<%= @shelter.id %>" method="post">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <input type="hidden" name="_method" value="PATCH" />
    <p>Name: </p>
    <input type="text" name="shelter[name]" value="<%= @shelter.name %>"/>
    <p>Address: </p>
    <input type="text" name="shelter[address]" value="<%= @shelter.address %>"/>
    <p>City: </p>
    <input type="text" name="shelter[city]" value="<%= @shelter.city %>"/>
    <p>State: </p>
    <input type="text" name="shelter[state]" value="<%= @shelter.state %>"/>
    <p>Zip: </p>
    <input type="text" name="shelter[zip]" value="<%= @shelter.zip %>"/>
    <input type='submit' name="submit_button"/>
  </form>

</div> /* final */
